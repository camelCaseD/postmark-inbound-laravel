    "license": "MIT",
    "keywords": [
        "postmark",
        "inbound",
        "laravel",
        "email",
        "parser"
    ],
    "authors": [
        {
            "name": "Brandon Borders",
            "email": "brandon@camelcased.com"
        }
    ],
    "require": {
