{
    "name": "camelcased/postmark-inbound-laravel",
    "description": "This is an Inbound Parser for Postmark meant to be used with Laravel.",
    "license": "MIT",
    "keywords": [
        "postmark",
        "inbound",
        "laravel",
        "email",
        "parser"
    ],
    "authors": [
        {
            "name": "Brandon Borders",
            "email": "brandon@camelcased.com"
        }
    ],
    "version": "0.4.0",
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.1.x"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Camelcased\\Postmark": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
      "post-update-cmd": [
        "echo Warning: Attachment::decodedContent has been changed to Attachment::DecodedContent"
      ]
    }
}
